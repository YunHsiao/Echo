        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                      ID_APP_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "撤消(&U)\tCtrl+Z",              ID_EDIT_UNDO
    END
    POPUP "图层(&I)"
    BEGIN
        MENUITEM "画刷层",                         ID_LAYOUTBRUSH
        MENUITEM "NPC层",                        ID_LAYOUTNPC
        MENUITEM "怪物层",                         ID_LAYOUTMONSTER
        MENUITEM "碰撞层",                         ID_LAYOUTCOLL
        MENUITEM "触发层",                         ID_LAYOUTTRIGGER
        MENUITEM "查看所有层",                       ID_LAYOUTALL
    END
    POPUP "地图(&M)"
    BEGIN
        MENUITEM "地图属性(&A)",                    ID_BACKSELECT
        MENUITEM "地图测试(&T)",                    ID_MAPTEST
        MENUITEM SEPARATOR
        MENUITEM "碰撞层插值渲染",                     ID_32818
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 MapEdit(&A)...",           ID_APP_ABOUT
    END
END


