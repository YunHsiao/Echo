IDR_MAINFRAME MENU 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建地图(&N)\tCtrl+N",            ID_FILE_NEW
        MENUITEM "打开地图(&O)...\tCtrl+O",         ID_FILE_OPEN
        MENUITEM "关闭地图(&C)\tCtrl+C",            ID_FILE_CLOSE
